{
  "name": "rsbuild-plugin-zip-file",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@10.7.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
